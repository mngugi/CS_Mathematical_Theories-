   "source": [
    "import numpy as np\n",
    "\n",
    "A = np.array([[3,8],[6,4]])\n",
    "\n",
    "result = A.T\n",
    "result_det = np.linalg.det(A)\n",
    "\n",
    "print(f'The transpose matrix is:{result} and the determinant measures is: {result_det}')"
